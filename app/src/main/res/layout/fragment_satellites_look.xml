<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.utente.logmyposition.fragments.SatellitesLookFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Azimuth"
        android:id="@+id/textView20"
        android:layout_gravity="left|top"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AzimuthVAlue"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginLeft="254dp"
        android:id="@+id/txtAzimuthSatLook"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elevation"
        android:id="@+id/textView24"
        android:layout_gravity="left|top"
        android:layout_marginLeft="0dp"
        android:layout_below="@+id/textView20"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ElevationValue"
        android:id="@+id/txtElevationSatLook"
        android:layout_below="@+id/txtAzimuthSatLook"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rotazione"
        android:id="@+id/textView26"
        android:layout_gravity="left|top"
        android:layout_marginLeft="0dp"
        android:layout_below="@+id/textView24" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rotazione"
        android:id="@+id/txtRotazionSatLook"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignParentEnd="true" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/txtRotazionSatLook"
        android:layout_centerHorizontal="true"
        android:id="@+id/ar_view_pane">

    </FrameLayout>

</RelativeLayout>
